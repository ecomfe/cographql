{
  "name": "cographql",
  "version": "0.0.1",
  "description": "GraphQL collaboration tools.",
  "main": "./dist/index.js",
  "bin": {
    "cographql": "./cli",
    "cgql": "./cli"
  },
  "scripts": {
    "lint": "fecs lib test/**/*.spec.js --type=js",
    "compile": "shx rm -rf dist && babel lib -d dist --copy-files",
    "test": "npm run compile && _mocha --compilers js:babel-core/register --recursive && npm run coverage",
    "coverage": "npm run compile && babel-node ./node_modules/.bin/babel-istanbul cover _mocha 'test/**/*.spec.@(js|es|es6)'",
    "coveralls": "shx cat ./coverage/lcov.info | coveralls",
    "watch": "babel lib -d dist --copy-files -w",
    "prepublish": "npm run compile",
    "dev": "cd example && set NODE_ENV=development && cgql init -s ./data/user.graphql"
  },
  "maintainers": [
    {
      "name": "Firede",
      "email": "firede@firede.us"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ecomfe/cographql.git"
  },
  "license": "MIT",
  "dependencies": {
    "antd": "^2.7.3",
    "autoprefixer": "^6.7.3",
    "babel-polyfill": "^6.20.0",
    "chalk": "^1.1.3",
    "deep-diff": "^0.3.4",
    "eslint-friendly-formatter": "^2.0.7",
    "graphiql": "^0.8.1",
    "graphql": "^0.8.2",
    "graphql-tools": "^0.10.0",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^2.3.0",
    "koa-convert": "^1.2.0",
    "koa-graphql": "^0.6.2",
    "koa-json": "^1.1.3",
    "koa-mount": "^2.0.0",
    "koa-onerror": "^3.0.2",
    "koa-router": "^7.0.1",
    "koa-static": "^2.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "redux": "^3.6.0",
    "reqwest": "^2.0.5",
    "rider": "^2.0.0",
    "rimraf": "^2.6.1",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-istanbul": "^0.12.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.1",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "fecs": "^1.1.0",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "koa-webpack-middleware": "^1.0.3",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.3.1",
    "shx": "^0.2.2",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.5.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.0"
  }
}
