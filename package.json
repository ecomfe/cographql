{
  "name": "cographql",
  "version": "0.0.1",
  "description": "GraphQL collaboration tools.",
  "main": "./dist/index.js",
  "bin": {
    "cographql": "./cli",
    "cgql": "./cli"
  },
  "scripts": {
    "lint": "fecs lib test/**/*.spec.js --type=js",
    "compile": "shx rm -rf dist && babel lib -d dist --copy-files",
    "test": "npm run compile && _mocha --compilers js:babel-core/register --recursive && npm run coverage",
    "coverage": "npm run compile && babel-node ./node_modules/.bin/babel-istanbul cover _mocha 'test/**/*.spec.@(js|es|es6)'",
    "coveralls": "shx cat ./coverage/lcov.info | coveralls",
    "watch": "babel lib -d dist --copy-files -w",
    "prepublish": "npm run compile"
  },
  "maintainers": [
    {
      "name": "Firede",
      "email": "firede@firede.us"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ecomfe/cographql.git"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "graphiql": "^0.8.1",
    "graphql": "^0.8.2",
    "graphql-tools": "^0.10.0",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-graphql": "^0.6.2",
    "koa-mount": "^2.0.0",
    "koa-onerror": "^3.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-istanbul": "^0.12.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "fecs": "^1.1.0",
    "mocha": "^3.2.0",
    "shx": "^0.2.2"
  }
}
